type Simple = {
	a: U8,
	b: U16,
	c: U32,
	d: U64
}

putVals: Simple take (a, b, c, d) -> Simple
putVals simple = simple { a = 0x12, b=0x3456, c = 0x7890abcd, d = 0xf0e1d2c3b4a59687}

getVals: Simple -> (Simple take (a, b, c, d), #Simple)
getVals simple {a=a', b=b', c=c', d=d'} = (simple, #{a=a', b=b', c=c', d=d'})