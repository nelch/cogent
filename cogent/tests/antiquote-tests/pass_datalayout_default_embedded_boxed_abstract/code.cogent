type X
type Y

genX : () -> X
genY : () -> Y

type Simple = {
	a: X,
	b: Y
}

putVals: Simple take (a, b) -> Simple
putVals simple = simple { a = genX (), b = genY () }

getVals: Simple -> (Simple take (a, b), #Simple)
getVals simple {a=a', b=b'} = (simple, #{a=a', b=b'})